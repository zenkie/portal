	#end
		#foreach($list in $allList)
            <li class="mThirCli">
                <a href="$list.get(0)" title="" class="mThirCell">
					<img src="$list.get(2)">
					<p>$stringutil.shorten($list.get(1),12)
					#if($ptype=="pdt")
						<br><em>ï¿¥$list.get(3)</em>
					#end
					</p>
				</a>
            </li>
		#end
    </ul>
</div>
#parse("/html/nds/oto/website/footer.vml")
</body>
</html>