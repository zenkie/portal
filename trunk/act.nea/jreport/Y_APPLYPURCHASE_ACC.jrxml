	<queryString><![CDATA[select     substr(MAX(sys_connect_by_path(comname, ',')), 2) comname ,
           substr(MAX(sys_connect_by_path(cper, ',')), 2) cper,
           substr(MAX(sys_connect_by_path(cper||comname, ',')), 2) aaa, 
            t.DOCNO ,t.PLANDATE,t.PRODATE ,t.DELIVERYDATE ,t.DELIVERYADDRESS ,t.MARKETDATE,t.tot_qty,t.CHECKSTD  ,t.fabric_weight ,t.remark , 
            cusname ,t.address,t.telephone, 
            matname 
   FROM  (  select  a.DOCNO ,a.PLANDATE,a.PRODATE,a.DELIVERYDATE,a.DELIVERYADDRESS,a.MARKETDATE,a.tot_qty,a.CHECKSTD,a.fabric_weight,a.remark, 
                    b.name as cusname,b.address ,b.telephone,
                    c.name as matname,
                    d.name as comname,
                    e.orderno,
                    DENSE_rank() over(partition by a.DOCNO ORDER BY e.id) rn,
                    decode(e.composition_per,'','',e.composition_per||'%') as cper   
            from    Y_MATERIAL_PLAN_SITEM  a, Y_CUSTOMER b,Y_MATERIAL c,Y_COMPOSITION d,Y_MATERIAL_ITEM e
            where   a.y_supplier_id =b.id(+)
            and     a.y_material_id = c.id
            and     c.id = e.y_material_id(+)
            and     e.y_composition_id = d.id(+)
            and     a.id = $P{objectid}
          ) t
   START WITH t.rn = 1
   CONNECT BY DOCNO = PRIOR DOCNO AND t.rn - 1 = PRIOR t.rn
   GROUP BY   t.DOCNO ,t.PLANDATE,t.PRODATE ,t.DELIVERYDATE ,t.DELIVERYADDRESS ,t.MARKETDATE,t.tot_qty,t.CHECKSTD ,t.fabric_weight ,t.remark , 
              cusname ,t.address,t.telephone,matname]]></queryString>

