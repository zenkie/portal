	<queryString><![CDATA[select a.docno,to_char(a.creationdate,'YYYY/MM/DD HH:MI:SS') as creationdate,e1.name as salename1,
e2.name as salename2,
c.name as storename,to_char(sysdate,'YYYY/MM/DD HH:MI:SS') as printtime,
c.phone,p.cardno ,c.address,t.name as stylename,u.value1_code as colorcode,p.integral,
u.value2_code as sizecode,b.priceactual,b.discount,b.qty,b.tot_amt_actual,a.description
from m_retail a,m_retailitem b,c_store c,hr_employee e1,c_vip p,m_product t,hr_employee e2,
m_attributesetinstance u
where a.id=b.m_retail_id 
and a.c_store_id=c.id
and a.salesrep_id=e1.id(+)
and b.salesrep_id=e2.id(+)
and a.c_vip_id=p.id(+)
and b.m_product_id=t.id
and b.m_attributesetinstance_id=u.id(+)
and a.id=$P{objectid}
group by t.name,u.value1_code,u.value2_code,p.integral,b.priceactual,b.discount,b.qty,b.tot_amt_actual,
a.description,b.id,a.docno,
a.creationdate,e1.name,c.name,e2.name,
c.phone,p.cardno ,c.address
order by t.name,u.value1_code,u.value2_code]]></queryString>

