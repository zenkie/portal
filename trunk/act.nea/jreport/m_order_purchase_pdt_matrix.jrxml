	<queryString><![CDATA[select '发货单' as BILLNAME,
a.DESCRIPTION,
a.BILLDATE,
a.DOCNO,
case a.DOCTYPE when 'POO' then '订单采购' else '正常采购' end as DOCTYPE,
case a.status when 1 then '(未提交)' when 2 then '(提交)' else '(待批)' end as STATUS,
case a.IN_STATUS when 1 then '未提交' when 2 then '提交' else '待批' end as IN_STATUS,
u1.TRUENAME as CREATERNAME,
a.CREATIONDATE,
u2.TRUENAME as MODIFIERNAME,
a.MODIFIEDDATE,
case a.STATUS when 2 then u3.TRUENAME else '未提交' end as STATUSERNAME,
a.STATUSTIME,
case a.IN_STATUS when 2 then u4.TRUENAME else '未入库' end as INERNAME,
a.INTIME,
b.name as STORNAME,
a.TOT_QTY,
a.TOT_QTYIN,
a.TOT_AMT_LIST,
a.TOT_AMT_ACTUAL,
a.TOT_AMTIN_ACTUAL,
c.name as SUPNAME
from m_purchase a,C_STORE b,C_SUPPLIER c,USERS u1,USERS u2,USERS u3,USERS u4
where a.C_STORE_ID =b.id
and a.C_SUPPLIER_ID =c.id
and a.OWNERID =u1.id(+)
and a.MODIFIERID =u2.id(+)
and a.STATUSERID =u3.id(+)
and a.INERID =u4.id(+)
and a.DOCTYPE='POO'
and a.id=$P{objectid}]]></queryString>

