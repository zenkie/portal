	<queryString><![CDATA[select rownum ,docno ,billdate,cusname,housename,deliverydate,brand,styleno, fmaterialcode,fmaterialname, kfname, 
colorcode, colorname, gstandard,caiunit,qty,useunit,purchase_rate,fprice, famount, REMARK
from (
select   a.docno, a.billdate, n.name as cusname, l.name as housename,
       TO_CHAR(TO_DATE(a.deliverydate, 'YYYYMMDD'), 'YYYY-MM-DD') as deliverydate,
       p.attribname as brand, m.name as styleno, c.code as fmaterialcode,
       c.name as fmaterialname, c.key_word as kfname, d.code as colorcode,
       d.name as colorname, k.name as gstandard, h.name as caiunit, b.qty,
       g.name as useunit, b.purchase_rate, b.fprice, b.famount, a.REMARK
from m_product m, y_customer n, y_warehouse l, y_purchase a, y_purchase_item b,
     y_material c, y_color d, y_unit g, y_unit h, y_spec k, M_DIM p
where b.y_material_id = c.id(+) and b.m_product_id = m.id(+) and
      b.y_purchase_id = a.id and b.y_Spec_Id = k.id(+) and
      a.y_supplier_id = n.id and a.y_warehouse_id = l.id and
      b.y_color_id = d.id(+) and b.purchase_unit_id = h.id(+) and
      b.use_unit_id = g.id(+) and a.m_dim1_id = p.id(+) and a.id = $P{objectid}
 union all
      select a.docno, a.billdate, n.name as cusname, l.name as housename,
       TO_CHAR(TO_DATE(a.deliverydate, 'YYYYMMDD'), 'YYYY-MM-DD') as deliverydate,
       p.attribname as brand, ' ' as   styleno, ' ' as fmaterialcode,
       b.name as fmaterialname, ' ' as kfname, ' ' as colorcode,
       ' ' as colorname, ' ' as gstandard, b.department as caiunit, round(qty, 2) qty,
       b.department as useunit, 0 aspurchase_rate, b.price fprice,
       price * round(qty, 2) famount, ' ' as REMARK
from y_purchase a, Y_PURCHASE_ACC_FEE b, y_customer n, y_warehouse l, M_DIM p
where b.y_purchase_acc_id = a.id and a.y_supplier_id = n.id and
      a.y_warehouse_id = l.id and a.m_dim1_id = p.id(+) and a.id = $P{objectid} 
 )  
order by rownum]]></queryString>

