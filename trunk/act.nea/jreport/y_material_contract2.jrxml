	</parameter>
	<queryString><![CDATA[select  substr(MAX(sys_connect_by_path(name3, ',')), 2) name3,
        substr(MAX(sys_connect_by_path(cper, ',')), 2) cper,
        substr(MAX(sys_connect_by_path(cper||name3, ',')), 2) aaa, 
        name1,name2,KEY_WORD,name9,name4,t.code,t.qty,t.fprice,tot_price,ddate,
        name6,t.CONTRACTNO,t.BILLDATE,t.bill_available_date,t.y_demander,t.address,t.description,t.PRINTCURRENCY,t.id as tid,
        t.remark,t.tot_famount_rmb
from(   select a.name as name1,b.name as name2,b.KEY_WORD,c.name as name3,k.name as name9,e.name as name4,f.code,
               d.qty,
               d.fprice,
               d.qty*d.fprice as tot_price,
               d.DELIVERYDATE as ddate,
               j.orderno,
               h.name as name6,g.CONTRACTNO,g.BILLDATE,g.bill_available_date,g.y_demander,g.address,g.description,g.PRINTCURRENCY,DENSE_rank() over(partition by d.id ORDER BY j.orderno) rn,
               decode(j.composition_per,'','',j.composition_per||'%') as cper,d.id,g.remark,get_money_rmb(g.tot_famount) as tot_famount_rmb         
        from   m_product a,y_material b,Y_COMPOSITION c,y_plan_item d,y_unit e,y_color f,y_plan g,y_customer h,Y_PLAN_ALLOT_ITEM i,y_material_item j,y_spec k
        where  d.y_material_id = b.id(+) 
        and    d.m_product_id = a.id (+)
        and    a.id = i.m_product_id(+)
        and    d.y_color_id = f.id
        and    d.y_plan_id = g.id
        and    g.y_supplier_id = h.id
        and    b.PURCHASE_UNIT_ID = e.id(+)
        and    j.y_Composition_Id = c.id(+)
        and    b.id = j.y_Material_Id(+)
        and    d.y_spec_id = k.id(+)
        and    g.id =$P{objectid}
        order by c.name,j.composition_per
     ) t 
start with t.rn = 1  
CONNECT BY ID = PRIOR ID AND t.rn - 1 = PRIOR t.rn             
group by name1,name2,KEY_WORD,name9,name4,t.qty,t.fprice,tot_price,ddate,t.code,
        name6,t.CONTRACTNO,t.BILLDATE,t.bill_available_date,t.y_demander,t.address,t.description,t.PRINTCURRENCY,t.id,t.remark,t.tot_famount_rmb]]></queryString>

