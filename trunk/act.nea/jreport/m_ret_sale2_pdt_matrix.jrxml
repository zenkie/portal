	<queryString><![CDATA[select case a.AU_STATE when 'A' then '已批-' when 'R' then '驳回-' else '待批-' end || '销售退货单(编号:' || a.DOCNO ||')' as BILLNAME,
a.DESCRIPTION,
e.name as RET_SALETYPE,
d1.name as CUSTOMER,
d2.name as CUSTOMERUP,
case a.RETSALETYPE when 'NOR' then '正常退货' when 'SPE' then '特殊(次品)退货' when 'DIF' then '差异退货'  else '调货退货' end as RETSALETYPE,
a.BILLDATE,
case a.status when 1 then '未提交' when 2 then '提交' else '待批' end as STATUS,
case a.OUT_STATUS when 1 then '未提交' when 2 then '已提交' else '待批' end as OUT_STATUS,
case a.IN_STATUS when 1 then '未提交' when 2 then '已提交' else '待批' end as IN_STATUS,
u1.TRUENAME as CREATERNAME,
a.CREATIONDATE,
u2.TRUENAME as MODIFIERNAME,
a.MODIFIEDDATE,
case a.STATUS when 2 then u3.TRUENAME else '未提交' end as STATUSERNAME,
a.STATUSTIME,
case a.OUT_STATUS when 2 then u4.TRUENAME else '未提交' end as OUTERNAME,
a.OUTTIME,
case a.IN_STATUS when 2 then u5.TRUENAME else '未提交' end as INERNAME,
a.INTIME,
b.name as STORNAME,
a.TOT_QTY,
a.TOT_AMT_LIST,
a.TOT_AMT_ACTUAL,
c.name as ORIG
from m_ret_sale a,C_STORE b,C_STORE c,C_CUSTOMER d1,C_CUSTOMER d2,C_RET_SALETYPE e, USERS u1,USERS u2,USERS u3,USERS u4,USERS u5
where a.C_STORE_ID =b.id
and a.C_ORIG_ID =c.id
and a.C_CUSTOMER_ID=d1.id(+)
and a.C_CUSTOMERUP_ID =d2.id(+)
and a.C_RET_SALETYPE_ID=e.id(+)
and a.OWNERID =u1.id(+)
and a.MODIFIERID =u2.id(+)
and a.STATUSERID =u3.id(+)
and a.OUTERID =u4.id(+)
and a.INERID =u5.id(+)
and a.id=$P{objectid}]]></queryString>

