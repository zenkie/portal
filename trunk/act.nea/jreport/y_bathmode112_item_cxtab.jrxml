(select b.m_product_id,b.m_attributesetinstance_id,b.QTY
from Y_REQUIRE_PLAN  a,Y_REQUIRE_PLAN_item b
where a.id=b.Y_REQUIRE_PLAN_id
and   a.status=2) a,m_attributesetinstance d,m_size e,m_color f
where t.m_product_id=a.m_product_id(+)
and t.m_attributesetinstance_id=a.m_attributesetinstance_id(+)
and   t.m_attributesetinstance_id=d.id(+)
and   d.value2_id=e.ID(+)
and   d.value1_id=f.id(+)
and t.m_product_id=$P{objectid1}
group by f.value ,e.value ,e.MARTIXCOL
order by f.value ,e.value ,e.MARTIXCOL]]></queryString>

