	<queryString><![CDATA[select type0,typecode0,type1,type2,name, code,KEY_WORD,prepareid,scd1,consumption,pro,remark,orderno,  
clrname||' '||scd1 as scd2 
from 
(select h.name as type0, h.code as typecode0, g.name as type1, b.name as type2,
       d.name, d.code,d.KEY_WORD,a.id as prepareid,       
       decode(b.material_type,'ACC',decode(a.is_size,'Y','配码','N',(select name from y_spec where id = a.y_spec_id))) as scd1,
       decode(a.is_size, 'Y', max(f.per_num), 'N', a.per_num) as consumption,
       decode(a.provider, '1', '公司', '2', '生产商') as pro, a.remark, a.orderno
from m_product c, y_product_bom_prepare a, y_pbp_size f, y_material d,
     y_material_category b, y_material_category g, y_material_category h
where c.id = a.style_id and a.material_id = d.id(+) and a.id = f.prepare_id(+) and
      d.y_material_category_id1 = h.id(+) and
      d.y_material_category_id2 = g.id(+) and
      d.y_material_category_id3 = b.id(+)      
      and c.id =$P{objectid}
group by d.code, d.name,d.KEY_WORD,a.id, h.name, h.code, g.name, b.name, a.is_size,
         a.y_spec_id, b.material_type, a.is_color, a.y_color_id, a.is_size,
         a.per_num, a.provider, a.remark, b.y_material_category_id, a.orderno ) T,
(select q.prepare_id, substr(max(sys_connect_by_path(q.name, ',')), 2) as clrname
from (select prepare_id, name, no + row_number() over(order by prepare_id) rn,
              row_number() over(partition by t.prepare_id ORDER BY t.name) rn1
       from (select distinct  t.prepare_id, clr.code, clr.name,
                              DENSE_rank() over(order by prepare_id) no
 from Y_PBP_COLOR t, y_color clr
              where t.y_color_id = clr.id and t.style_id = $P{objectid} order by prepare_id) t) q
start with q.rn1 = 1
connect by q.rn - 1 = prior q.rn
group by q.prepare_id) K
where T.prepareid=K.prepare_id(+)        
order by typecode0, orderno]]></queryString>

