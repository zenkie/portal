	<queryString><![CDATA[select  substr(MAX(sys_connect_by_path(comname, ',')), 2) comname,
        substr(MAX(sys_connect_by_path(cper, ',')), 2) cper,
         substr(MAX(sys_connect_by_path(cper||comname, ',')), 2) aaa, 
           t.DOCNO ,t.PLANDATE,t.PRODATE ,t.DELIVERYDATE ,t.DELIVERYADDRESS ,t.MARKETDATE,t.tot_qty,t.CHECKSTD ,t.fabric_width ,t.fabric_weight ,t.remark , 
           cusname ,t.address,t.telephone, 
           matname,t.matcode,t.spec,t.key_word 
   FROM  (  select  a.DOCNO ,a.PLANDATE,a.PRODATE,a.DELIVERYDATE,a.DELIVERYADDRESS,a.MARKETDATE,a.tot_qty,a.CHECKSTD,
                   /*a.fabric_width*/ (case when a.fabric_width = 0.00 then '不知道' else to_char(a.fabric_width) end) as fabric_width,
                   a.fabric_weight,a.remark, 
                   (case  when b.name is null then '不知道' else b.name end )as cusname,
                   (case when b.address is null then '不知道' else b.address end) as address,
                   (case when b.telephone is null then '不知道' else b.telephone end) as telephone,
                    c.name as matname,c.code as matcode,
                    (case when d.name is null then '不知道' else d.name end) as comname,
                    e.orderno,
                    DENSE_rank() over(partition by a.DOCNO ORDER BY e.id) rn,
                    decode(e.composition_per,'','',e.composition_per||'%') as cper,
                    c.key_word,(case when c.spec is null then '不知道' else c.spec end) as spec  
            from    Y_MATERIAL_PLAN_SITEM  a, Y_CUSTOMER b,Y_MATERIAL c,Y_COMPOSITION d,Y_MATERIAL_ITEM e
            where   a.y_supplier_id = b.id(+)
            and     a.y_material_id =c.id(+)
            and     c.id= e.y_material_id(+)
            and     e.y_composition_id = d.id(+)
            and     a.id = $P{objectid}
          ) t
   START WITH t.rn = 1
   CONNECT BY DOCNO = PRIOR DOCNO AND t.rn-1  = PRIOR t.rn
   GROUP BY   t.DOCNO ,t.PLANDATE,t.PRODATE ,t.DELIVERYDATE ,t.DELIVERYADDRESS ,t.MARKETDATE,t.tot_qty,t.CHECKSTD ,t.fabric_width ,t.fabric_weight ,t.remark , 
              cusname ,t.address,t.telephone,
              matname,t.spec,t.key_word,t.matcode]]></queryString>

