	<queryString><![CDATA[select case a.IN_STATUS
         when 2 then
          '销售退货单'
         else
          '销售退货单（未验收）'
       end as billname,
       a.docno,
       case
         when a.IN_STATUS = 2 then
          to_date(a.datein, 'yyyy-mm-dd')
         when a.OUT_STATUS = 2 and a.IN_STATUS <> 2 then
          to_date(a.dateout, 'yyyy-mm-dd')
         else
          to_date(a.billdate, 'yyyy-mm-dd')
       end as billdate,
       nvl(a.description, ' ') as description,
       b1.name as origname,
       b2.name as destname,
       b2.address as inaddress,
       nvl(e1.name, '') as OWNERID,
       a.CREATIONDATE,
       nvl(e2.name, '') as STATUSERID,
       a.STATUSTIME,
       nvl(e3.name, '') as OUTERID,
       a.OUTTIME,
       nvl(e4.name, '') as INERID,
       a.INTIME,
       b1.CARRIER
  from M_RET_SALE a,
       c_store    b1,
       c_store    b2,
       USERS      e1,
       USERS      e2,
       USERS      e3,
       USERS      e4
 where a.c_orig_id = b1.id
   and a.c_store_id = b2.id
   and a.ownerid = e1.id(+)
   and a.statuserid = e2.id(+)
   and a.OUTERID = e3.id(+)
   and a.INERID = e4.id(+)
   and a.id = $P{objectid}]]></queryString>

