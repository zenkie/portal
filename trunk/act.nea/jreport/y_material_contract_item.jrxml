	<parameter name="objectid1" isForPrompting="false" class="java.lang.Integer">
		<parameterDescription><![CDATA[objectid1]]></parameterDescription>
	</parameter>
	<queryString><![CDATA[select  substr(MAX(sys_connect_by_path(name3, ',')), 2) name3,
        substr(MAX(sys_connect_by_path(cper, ',')), 2) cper,
        substr(MAX(sys_connect_by_path(cper||name3, ',')), 2) aaa, 
        name1,name2,t.KEY_WORD,t.spec,t.factorycolor,t.fabric_width,name4,t.code,t.qty,t.fprice,tot_price,t.DELIVERYDATE,/*name5,t.CONTRACTNO,t.BILLDATE,
        t.address,t.y_demander,*/t.BILL_AVAILABLE_DATE,t.description,t.PRINTCURRENCY,t.remark,t.tot_famount_rmb 
from(  select a.name as name1,b.name as name2,b.KEY_WORD,b.spec,d.factorycolor,c.name as name3,d.fabric_width,e.name as name4,f.code,d.qty,d.fprice,d.qty*d.fprice as tot_price,d.DELIVERYDATE,g.remark,
              /*h.name as name5,g.CONTRACTNO,g.BILLDATE,g.address,g.y_demander,*/g.BILL_AVAILABLE_DATE,g.description,g.PRINTCURRENCY,j.orderno,DENSE_rank() over(partition by d.fprice ORDER BY j.orderno,c.name) rn,
              decode(j.composition_per,'','',j.composition_per||'%') as cper,get_money_rmb(g.tot_famount) as tot_famount_rmb     
       from   m_product a,y_material b,Y_COMPOSITION c,y_plan_item d,y_unit e,y_color f,y_plan g,y_customer h,Y_PLAN_ALLOT_ITEM i,y_material_item j
       where  d.y_material_id = b.id(+) 
       and    d.m_product_id = a.id (+)
       and    a.id = i.m_product_id(+)
       and    d.y_color_id = f.id 
       and    d.y_plan_id = g.id  
       and    g.y_supplier_id = h.id
       and    b.PURCHASE_UNIT_ID = e.id
       and    j.y_Composition_Id = c.id(+)
       and    b.id = j.y_Material_Id(+) 
       and    g.id = $P{objectid1}
       order by c.name,j.composition_per
     ) t
start with t.rn = 1  
CONNECT BY FPRICE = PRIOR FPRICE AND t.rn - 1 = PRIOR t.rn     
group by name1,name2,KEY_WORD,spec,factorycolor,t.fabric_width,name4,t.code,t.qty,t.fprice,tot_price,t.DELIVERYDATE,/*name5,t.CONTRACTNO,t.BILLDATE,
          t.address,t.y_demander,*/t.BILL_AVAILABLE_DATE,t.description,t.PRINTCURRENCY,t.remark,t.tot_famount_rmb]]></queryString>

