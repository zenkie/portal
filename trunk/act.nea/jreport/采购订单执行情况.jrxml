     a4."ATTRIBNAME" AS b3,
     SUM(FA_STORAGE.QTYPO),
     SUM(FA_STORAGE.QTYPPURCHASE)
FROM
     "FA_STORAGE" FA_STORAGE,
     "M_DIM" a4,
     "M_DIM" a2,
     "M_PRODUCT" a1,
     "M_DIM" a3,
     "C_STORE" a0
