 AND (a1.ID(+) = FA_STORAGE.M_PRODUCT_ID)
 AND (a2.ID(+) = a1.M_DIM1_ID)
 AND (a3.ID(+) = a1.M_DIM3_ID)
 AND (a4.ID(+) = a1.M_DIM6_ID)
GROUP BY
     a0.NAME,
     a2.ATTRIBNAME,
     a3.ATTRIBNAME,
     a4.ATTRIBNAME
