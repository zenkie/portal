	<queryString><![CDATA[select case a.STATUS
         when 2 then
          b.QTYOUT
         else
          b.QTY
       end as QTY,
       c.name as productname,
       c.value as ass,
       e.value as value1,
       f.NAME  as colorname,
       f.value as value2,
       replace(e.MARTIXCRON, '</BR>', chr(13)) as MARTIXCRON,
       to_char(b.pricelist, '999999.99') as pricelist,
       case a.STATUS
         when 2 then
          b.TOT_AMTOUT_LIST
         else
          b.TOT_AMT_LIST
       end as amt,
       c.value as value5,
       c.serialno
  from M_OUT                  a,
       M_OUTitem              b,
       m_product              c,
       m_attributesetinstance d,
       m_size                 e,
       m_color                f
 where a.id = b.M_OUT_id
   and b.m_product_id = c.id
   and b.m_attributesetinstance_id = d.id
   and e.ID = d.value2_id
   and f.id = d.value1_id
   and b.M_OUT_id = $P{objectid1}
 ORDER BY e.MARTIXCRON]]></queryString>

