	<parameter name="objectid1" isForPrompting="false" class="java.lang.Integer">
		<parameterDescription><![CDATA[objectid1]]></parameterDescription>
	</parameter>
	<queryString><![CDATA[select distinct substr(MAX(sys_connect_by_path(to_char(value1_code), ',')), 2) aa ,
 t1.productname,
       t1.productvalue,
       t1.UNIT,
       max(t1.deliverydate) as deliverydate,
       max(t1.composition) as composition,
       max(t2.PRCFPRICE) as PRCFPRICE,sum(t1.qty) as qty,
      sum(t1.QTY* t2.PRCFPRICE)  as PRCFAMOUNT
  from (select b.M_PRODUCT_ID,
               c.name as productname,
               c.value as productvalue,
               c.UNIT,
               max(b.deliverydate) as deliverydate,
               max(b.composition) as composition,
               sum(b.QTY) as QTY,
               DENSE_rank() over(partition by c.name ORDER BY d.value1_code ) rn,
               d.value1_code as value1_code
          from Y_PRODUCE_ITEM         b,
               m_product              c,
               m_attributesetinstance d
         where b.m_product_id = c.id
           and b.m_attributesetinstance_id = d.id
          and b.Y_PRODUCE_id = $P{objectid1}
         group by b.M_PRODUCT_ID,d.value1_code,
                  c.name,
                  c.value,
                  c.UNIT) t1,
       (select M_PRODUCT_ID, sum(e.FPRICE) as PRCFPRICE
          from Y_PRODUCE_PRC_ITEM e
         where e.Y_PRODUCE_id = $P{objectid1}
         GROUP BY M_PRODUCT_ID) t2
         where t1.M_PRODUCT_ID = t2.M_PRODUCT_ID
   START WITH t1.rn = 1
   CONNECT BY productname = PRIOR productname AND t1.rn - 1 = PRIOR t1.rn    
    group by t1.productname,
       t1.productvalue,
       t1.UNIT]]></queryString>

