	<queryString><![CDATA[select a.docno,
         a.billdate,
       nvl(to_char(a.dateout), ' ') as dateout,
       nvl(a.description, ' ') as description,
       b.DESCRIPTION as origname,
       b1.DESCRIPTION as destname,
       case a.STATUS
           when 2 then
            u3.NAME
           else
            '未入库'
       end as INERNAME,
       a.INTIME,
       u1.NAME as CREATERNAME,
       a.CREATIONDATE,
       decode(billtype,
              'M_SALEOUT',
              '销售出库',
              'M_RET_SALEOUT',
              '销售退货出库',
              'M_TRANSFEROUT',
              '调拨出库',
              'M_V_CUSRET_PUROUT',
              '经销商采购退货出库',
              'M_RET_PUROUT',
              '采购退货出库'
          ) as billtype,
       e.name,
       a.TRANWAY_PAY,
       a.tranwayno,   
       a.receiver_phone,
       a.weightorsize,
       a.dest_addr,
       a.consignee,
       u4.name as outname,
       a.OUTTIME
  from M_OUT               a,
       c_store             b,
       c_store             b1,
       AD_LIMITVALUE_GROUP c,
       AD_LIMITVALUE       d,
       c_tranway_jz e,
       USERS               u1,
       USERS               u2,
       USERS               u3,
       users               u4
       
 where a.c_orig_id = b.id
   and c.id = d.ad_limitvalue_group_id
   and c.name = 'BILLTYPE(M_OUT)'
   and a.BILLTYPE = d.value
   and b1.name = a.destname
   and a.OWNERID = u1.id(+)
   and a.STATUSERID = u2.id(+)
   and a.INERID = u3.id(+)
   and a.OUTERID =u4.id(+)
   and a.c_tranway_jz_id=e.id(+)
   and a.id = $P{objectid}]]></queryString>

