	<queryString><![CDATA[select t1.productname,t1.productvalue,t1.colorcode,t1.colorname,t1.UNIT,
t1.QTY,t2.PRCFPRICE,t1.QTY*t2.PRCFPRICE as PRCFAMOUNT
from
(select c.name as productname,c.value as productvalue,f.value as colorcode,f.name as colorname,c.UNIT,
sum(b.QTY) as QTY
from Y_INFORM_ITEM b,m_product c,m_attributesetinstance d,M_COLOR f
where b.m_product_id=c.id
and b.m_attributesetinstance_id=d.id
and d.value1_id=f.id
and b.Y_INFORM_id=$P{objectid1} 
group by c.name,c.value,f.value,f.name,c.UNIT
) t1,
(select sum(e.FPRICE) as PRCFPRICE
from Y_INFORM_PRC_ITEM e
where e.Y_INFORM_id=$P{objectid1} 
) t2
ORDER BY t1.productname,t1.colorcode]]></queryString>

