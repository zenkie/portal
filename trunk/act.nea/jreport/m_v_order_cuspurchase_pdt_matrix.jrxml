	<queryString><![CDATA[select case a.AU_STATE when 'A' then '已批-' when 'R' then '驳回-' else '待批-' end || '经销商订单采购' as BILLNAME,
a.DESCRIPTION,
a.DOCNO,
case a.DOCTYPE when 'SUO' then '直接销售' when 'SOO' then '期货订单发货' else '现货订单发货' end as DOCTYPE,
case a.SALETYPE when 'NOR' then '正常采购' when 'SPE' then '特殊(换货)采购' when 'ORD' then '订单采购' when 'TRA' then '调货采购' else '差异采购' end as SALETYPE,
a.BILLDATE,
case a.status when 1 then '(未提交)' when 2 then '(提交)' else '(待批)' end as STATUS,
case a.OUT_STATUS when 1 then '未提交' when 2 then '已提交' else '待批' end as OUT_STATUS,
case a.IN_STATUS when 1 then '未提交' when 2 then '已提交' else '待批' end as IN_STATUS,
u1.TRUENAME as CREATERNAME,
a.CREATIONDATE,
u2.TRUENAME as MODIFIERNAME,
a.MODIFIEDDATE,
case a.STATUS when 2 then u3.TRUENAME else '未提交' end as STATUSERNAME,
a.STATUSTIME,
case a.OUT_STATUS when 2 then u4.TRUENAME else '未提交' end as OUTERNAME,
a.OUTTIME,
case a.IN_STATUS when 2 then u5.TRUENAME else '未提交' end as INERNAME,
a.INTIME,
b.name as STORNAME,
a.TOT_QTY,
a.TOT_AMT_LIST,
a.TOT_AMT_ACTUAL,
c.name as DEST
from M_SALE a,C_STORE b,C_STORE c,USERS u1,USERS u2,USERS u3,USERS u4,USERS u5
where a.C_STORE_ID =b.id
and a.C_DEST_ID =c.id
and a.OWNERID =u1.id(+)
and a.MODIFIERID =u2.id(+)
and a.STATUSERID =u3.id(+)
and a.OUTERID =u4.id(+)
and a.INERID =u5.id(+)
and a.id=$P{objectid}]]></queryString>

